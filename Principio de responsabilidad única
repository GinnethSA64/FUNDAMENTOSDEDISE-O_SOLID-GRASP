'sin principio
public class Coche 
    
    PUBLIC marca AS String
    public modelo as string
    public precio as string

    getMarcaCoche as string()
    getModeloCoche as string()
    getPrecioCoche as string()

    MessageBox.show ("La marca del coche" & marca)
    MessageBox.show ("El modelo del coche" & modelo)
    MessageBox.show ("El precio del coche" & precio)

end class



'con principio
public class Coche 
    
    PUBLIC marca AS String
    public modelo as string
    public precio as string

    getMarcaCoche as string()
    getModeloCoche as string()
    getPrecioCoche as string()

end class

public sub MostrarMarcaCoche
MessageBox.show ("La marca del coche" & marca)
end sub

public sub MostrarModeloCoche 
MessageBox.show ("El modelo del coche" & modelo)  
end sub

public sub MostrarPrecioCoche 
MessageBox.show ("El modelo del coche" & precio)  
end sub